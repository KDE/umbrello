#: duchain/builders/contextbuilder.cpp:397
#, kde-format
msgid "Circular inheritance of %1 and %2"
msgstr "Зацикливание наследования %1 и %2"

#: duchain/builders/declarationbuilder.cpp:321
#, kde-format
msgid "Access type for interface method %1 must be omitted."
msgstr "Тип доступа к интерфейсному методу %1 не должен указываться."

#: duchain/builders/declarationbuilder.cpp:325
#, kde-format
msgid "Interface function %1 cannot contain body."
msgstr "Интерфейсная функция %1 не может содержать тело."

#: duchain/builders/declarationbuilder.cpp:337
#, kde-format
msgid ""
"Class %1 contains abstract method %2 and must therefore be declared abstract "
"or implement the method."
msgstr ""
"Класс %1 содержит абстрактный метод %2, поэтому он должен быть абстрактным "
"(или реализовывать этот метод)."

#: duchain/builders/declarationbuilder.cpp:343
#, kde-format
msgid "Abstract function %1 cannot contain body."
msgstr "Абстрактная функция %1 не может иметь тело."

#: duchain/builders/declarationbuilder.cpp:346
#, kde-format
msgid "Cannot use the final modifier on an abstract class member."
msgstr "У члена абстрактного класса не может быть модификатора final."

#: duchain/builders/declarationbuilder.cpp:356
#, kde-format
msgid "Non-abstract method %1 must contain body."
msgstr "Не абстрактный метод %1 должен иметь тело."

#: duchain/builders/declarationbuilder.cpp:374
#, kde-format
msgid "Properties cannot be declared final."
msgstr "Свойства не могут иметь модификатора final."

#: duchain/builders/declarationbuilder.cpp:377
#, kde-format
msgid "Properties cannot be declared abstract."
msgstr "Свойства не могут быть абстрактными."

#: duchain/builders/declarationbuilder.cpp:423
#, kde-format
msgid ""
"Trait method %1 has not been applied, because there are collisions with "
"other trait methods on %2"
msgstr ""
"Метод признаков %1 не был применён из-за наличия конфликтов с другими "
"методами признаков в %2"

#: duchain/builders/declarationbuilder.cpp:577
#, kde-format
msgid "Cannot redeclare private property %1 from this context."
msgstr "Переопределить закрытое свойство %1 в данном контексте нельзя."

#: duchain/builders/declarationbuilder.cpp:583
#, kde-format
msgid "Cannot redeclare protected property %1 from this context."
msgstr "Переопределить защищённое свойство %1 в данном контексте нельзя."

#: duchain/builders/declarationbuilder.cpp:616
#, kde-format
msgid "Traits cannot have constants."
msgstr "Типажи не могут содержать константы."

#: duchain/builders/declarationbuilder.cpp:657
#, kde-format
msgid ""
"Only booleans, ints, floats and strings are allowed for class constants."
msgstr ""
"Константы в классах могут быть только булевыми, целочисленными, числами с "
"плавающий точкой и строками."

#: duchain/builders/declarationbuilder.cpp:709
#, kde-format
msgid "Cannot use 'final' as method modifier"
msgstr "Использовать «final» в качестве модификатора метода нельзя."

#: duchain/builders/declarationbuilder.cpp:712
#, kde-format
msgid "Cannot use 'static' as method modifier"
msgstr "Использовать «static» в качестве модификатора метода нельзя."

#: duchain/builders/declarationbuilder.cpp:760
#, kde-format
msgid "Default value for parameters with a class type hint can only be NULL."
msgstr ""
"Значением по умолчанию для параметров с контролем типа может быть только "
"NULL."

#: duchain/builders/declarationbuilder.cpp:763
#, kde-format
msgid "Following parameters must have a default value assigned."
msgstr "Для следующих параметров должно быть задано значение по умолчанию."

#: duchain/builders/declarationbuilder.cpp:870
#, kde-format
msgid "Cannot redeclare PHP internal %1."
msgstr "Переопределить внутреннее объявление PHP %1 нельзя."

#: duchain/builders/declarationbuilder.cpp:873
#, kde-format
msgid ""
"%1 and %2 define the same property (%3) in the composition of %1. This might "
"be incompatible, to improve maintainability consider using accessor methods "
"in traits instead."
msgstr ""
"%1 и %2 определяют одно и то же свойство (%3) в составе %1. Это может "
"повлиять на совместимость программы с интерпретаторами языка. Чтобы сделать "
"сопровождение программы удобнее, рекомендуется использовать методы доступа в "
"типажах."

#: duchain/builders/declarationbuilder.cpp:881
#, kde-format
msgid "Cannot redeclare %1, already declared in %2 on line %3."
msgstr "Переопределить %1 нельзя (уже определено в %2, строке %3)."

#: duchain/builders/declarationbuilder.cpp:967
#, kde-format
msgid ""
"Class %1 contains %2 abstract methods and must therefore be declared "
"abstract or implement the remaining methods."
msgstr ""
"Класс %1 содержит абстрактные методы (%2), поэтому он должен быть "
"абстрактным (или реализовывать остальные методы)."

#: duchain/builders/declarationbuilder.cpp:974
#, kde-format
msgid "Cannot re-assign $this."
msgstr "Переназначить $this нельзя."

#: duchain/builders/declarationbuilder.cpp:1410
#, kde-format
msgid "The use statement with non-compound name '%1' has no effect."
msgstr "Оператор use с не составным именем %1 не имеет смысла."

#: duchain/builders/declarationbuilder.cpp:1434
#, kde-format
msgid "Cannot use '%1' as '%2' because the name is already in use."
msgstr "Использовать «%1» как «%2» нельзя, так как это имя уже используется."

#: duchain/builders/usebuilder.cpp:191
#, kde-format
msgid "Usage of %1 is deprecated."
msgstr "Использование %1 считается устаревшим."

#: duchain/builders/usebuilder.cpp:193
#, kde-format
msgid "Declaration not found: %1"
msgstr "Объявление не найдено: %1"

